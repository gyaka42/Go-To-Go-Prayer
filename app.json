{
  "expo": {
    "name": "Go-To-Go Prayer",
    "slug": "go-to-go-prayer",
    "scheme": "gotogoprayer",
    "icon": "./assets/images/Dark/AppIcon~ios-marketing.png",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-font",
      [
        "expo-notifications",
        {
          "sounds": ["./assets/sounds/adhan_short.wav"]
        }
      ]
    ],
    "ios": {
      "icon": "./assets/images/Dark/AppIcon~ios-marketing.png",
      "buildNumber": "1",
      "supportsTablet": true,
      "bundleIdentifier": "com.gogo22.gotogoprayer",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Je locatie wordt gebruikt om nauwkeurige gebedstijden te berekenen.",
        "NSUserNotificationsUsageDescription": "Meldingen worden gebruikt om je op gebedstijden te attenderen.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "229fd9a3-2505-4801-9e23-a988563d72f1"
      }
    }
  }
}
